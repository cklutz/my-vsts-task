{
    "externals": {
        "nugetv2": [
            {
                "name": "VstsTaskSdk",
                "version": "0.10.0",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                        "source": [
                            "*.dll",
                            "*.ps1",
                            "*.psd1",
                            "*.psm1",
                            "lib.json",
                            "Strings"
                        ],
                        "dest": "ps_modules/VstsTaskSdk/",
                        "options": "-R"
                    }
                ]
            },
            {
                "name": "OpenCover",
                "version": "4.6.519",
                "repository": "https://www.nuget.org/api/v2/",
                "cp": [
                    {
                        "source": [
                            "tools/*.*"
                        ],
                        "dest": "tools/OpenCover/",
                        "options": "-R"
                    }
                ]
            },
            {
                "name": "ReportGenerator",
                "version": "2.4.4.0",
                "repository": "https://www.nuget.org/api/v2/",
                "cp": [
                    {
                        "source": [
                            "tools/*.*"
                        ],
                        "dest": "tools/ReportGenerator/",
                        "options": "-R"
                    }
                ]
            },
            {
                "name": "OpenCoverToCoberturaConverter",
                "version": "0.2.6.0",
                "repository": "https://www.nuget.org/api/v2/",
                "cp": [
                    {
                        "source": [
                            "tools/*.*"
                        ],
                        "dest": "tools/OpenCoverToCoberturaConverter/",
                        "options": "-R"
                    }
                ]
            }
        ]
    }
}
